{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash $CLAUDE_PROJECT_DIR/.claude/hooks/session-start.sh",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "extraKnownMarketplaces": {
    "task-orchestrator-marketplace": {
      "source": {
        "source": "directory",
        "path": "./claude-plugins/task-orchestrator"
      }
    }
  },
  "enabledPlugins": {
    "task-orchestrator@task-orchestrator-marketplace": true
  }
}
